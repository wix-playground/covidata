{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react-native",
    "esModuleInterop": true,
    "noEmit": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@prod-e2e/*": ["./production-e2e/*"],
    }
  },
  "include": [
    "./typings/*",
    "./**/*.ts*",
  ],
}
