{
  "name": "covidata",
  "version": "0.0.1",
  "private": true,
  "main": "./src/module.ts",
  "scripts": {
    "clear-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && rm package-lock.json && npm cache verify && npm install && npm start -- --reset-cache",
    "test": "TZ=UTC jest src/__tests__/*.spec.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pretty": "prettier --write ./src ./demo ./e2e ./fake-server",
    "fake-server": "node fake-server",
    "start-e2e": "RN_SRC_EXT=e2e.js react-native start",
    "test-e2e": "detox test --configuration ios.sim.debug",
    "postinstall": "one-app-engine --setup",
    "start": "one-app-engine --mock-mode quickLogin -ia —force-localhost",
    "start-admin": "one-app-engine --mock-mode quickLogin -ia —force-localhost --app admin",
    "start-ios": "one-app-engine --mock-mode quickLogin -i —force-localhost",
    "start-android": "one-app-engine --mock-mode quickLogin -a —force-localhost",
    "release": "mltci release",
    "customPublish": "mltci release"
  },
  "oneAppEngine": {
    "places": {
      "platformModule": "@wix/wix-one-app-platform",
      "modules": [
        "template-demo-module",
        "covidata",
        "@wix/wix-one-app-platform"
      ]
    },
    "admin": {
      "platformModule": "@wix/wix-owners-app-platform",
      "modules": [
        "template-demo-module",
        "covidata",
        "@wix/wix-owners-app-platform"
      ],
      "tabs": [
        "dashboard"
      ]
    }
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "7.12.13",
    "country-to-emoji-flag": "1.0.0",
    "http": "0.0.1-security",
    "jetifier": "1.6.8",
    "prop-types": "15.7.2",
    "react-native-chart-kit": "5.6.1",
    "react-native-testing-library": "1.14.0",
    "react-native-ui-lib": "5.24.2",
    "react-native-windows": "0.61.16",
    "react-redux": "7.2.4",
    "redux": "4.1.0",
    "redux-component-driver": "0.10.2",
    "redux-thunk": "2.3.0",
    "dateformat": "^4.5.1",
    "wix-mobile-lifecycle-tools": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.10.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.12",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@wix/wix-one-app-platform": "ga",
    "@wix/wix-owners-app-platform": "ga",
    "@wix/wix-react-native-ui-lib": "ga",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "detox": "^16.7.1",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react": "^16.11.0",
    "react-component-driver": "^0.10.0",
    "react-test-renderer": "16.11.0",
    "react-test-renderer-utils": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "tslint": "6.1.3",
    "typescript": "^3.8.3",
    "wix-one-app-engine": "ga"
  },
  "peerDependencies": {
    "wix-one-app-engine": "*"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "module:metro-react-native-babel-preset"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties"
        ],
        "retainLines": true
      }
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "node_modules/react-native/.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "node_modules/react-native-chart-kit/.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "./jest.init.js"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/CoviData.app",
        "build": "xcodebuild -workspace ios/CoviData.xcworkspace -scheme CoviData -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_3a_API_28"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_3a_API_28"
        }
      }
    },
    "test-runner": "jest"
  }
}
